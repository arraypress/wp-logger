{
  "name": "arraypress/wp-logger",
  "version": "2.0.0",
  "description": "A simple, lean logging library for WordPress plugins and themes with registry pattern support.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-logger/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Logger\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-logger/issues",
    "source": "https://github.com/arraypress/wp-logger"
  },
  "keywords": [
    "wordpress",
    "logging",
    "debug",
    "wp",
    "arraypress",
    "error-handling",
    "exceptions",
    "registry"
  ]
}